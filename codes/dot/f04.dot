graph g{
layout = dot

u [shape=circle];
v [shape=circle];
w [shape=circle];
x [shape=circle];
y [shape=circle];

u -- v [label = "∞"];
u -- w [label = "-2", style = dotted];
u -- x [label = "-1", style = dotted];

v -- w [label = "-1", style = dotted];
v -- x [label = "-∞", style = dotted];

w -- x [label = "5"];
w -- y [label = "-1", style = dotted];

x -- y [label = "-5", style = dotted];
}